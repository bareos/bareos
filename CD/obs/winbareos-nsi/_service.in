<?xml version="1.0"?>
<services>
  <service name="obs_scm">
    <param name="revision">@REVISION@</param>
    <param name="@VERSION_OR_VERSIONPREFIX@">@VERSION_NUMBER@</param>
    <param name="url">@GIT_URL@</param>
    <param name="submodules">disable</param>
    <param name="filename">bareos</param>
    <param name="scm">git</param>
    <param name="subdir">core</param>
    <param name="extract">LICENSE</param>
    <param name="extract">platforms/win32/winbareos-nsi.spec</param>
    <param name="extract">platforms/win32/fillup.sed</param>
    <param name="extract">platforms/win32/clientdialog.ini</param>
    <param name="extract">platforms/win32/databasedialog.ini</param>
    <param name="extract">platforms/win32/directordialog.ini</param>
    <param name="extract">platforms/win32/bareos.icog</param>
    <param name="extract">platforms/win32/bareosdir.icog</param>
    <param name="extract">platforms/win32/bareosfd.icog</param>
    <param name="extract">platforms/win32/bareossd.icog</param>
    <param name="extract">platforms/win32/bareostools.icog</param>
    <param name="extract">platforms/win32/bconsole.icog</param>
    <param name="extract">platforms/win32/bsmtp.icog</param>
    <param name="extract">platforms/win32/traymon.icog</param>
    <param name="extract">platforms/win32/winbareos.nsi</param>
  </service>
  <service mode="buildtime" name="tar" />
  <service mode="buildtime" name="recompress">
    <param name="file">*.tar</param>
    <param name="compression">gz</param>
  </service>
  <service name="set_version"/>
</services>
