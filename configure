                  MYSQL_LIBDIR=/usr/local/lib64
               else
                  MYSQL_LIBDIR=/usr/local/lib
               fi
               MYSQL_BINDIR=/usr/local/bin
            else
               { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
               as_fn_error $? "Unable to find mysql.h in standard locations" "$LINENO" 5
            fi
         fi
