Storage {
  Name = ndmp
  Address = @ndmp_tape_and_robot_agent_address@
  Port = 10000
  Protocol = NDMPv4
  Auth Type = @ndmp_tape_and_robot_agent_auth_type@
  Username = "@ndmp_tape_and_robot_agent_user@"
  Password = "@ndmp_tape_and_robot_agent_password@"
  Maximum Concurrent Jobs = 1
  Autochanger = yes
  MediaType = NDMP-Tape
  # use "status storage" to determine the tape and changer devices
  NDMP Changer Device = "@ndmp_tape_and_robot_agent_changer_device@"
  @ndmp_tapes_devices@
}
