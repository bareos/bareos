JobDefs {
  Name = "DefaultJob"
  Type = Backup
  Level = Incremental
  Storage = File
  Messages = Standard
  Pool = Incremental
  Priority = 10
  Fileset = Default
  Write Bootstrap = "@working_dir@/%c.bsr"
  Full Backup Pool = Full                  # write Full Backups into "Full" Pool
  Incremental Backup Pool = Incremental    # write Incr Backups into "Incremental" Pool
}
