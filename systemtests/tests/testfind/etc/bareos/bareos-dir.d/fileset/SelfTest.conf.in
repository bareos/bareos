FileSet {
  Name = "SelfTest"
  Description = "fileset just to backup some files for selftest"
  Include {
    Options {
      Signature = XXH128
      fstype = ext2
      fstype = ext3
      fstype = ext4
      fstype = overlay
      fstype = jfs
      fstype = ufs
      fstype = xfs
      fstype = zfs
      fstype = btrfs
    }
   File=<@tmpdir@/file-list
  }

  Exclude {
   File = "@tmpdir@/data/build/src/tests"
  }
}
