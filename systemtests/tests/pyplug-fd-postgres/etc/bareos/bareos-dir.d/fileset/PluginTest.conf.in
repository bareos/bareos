FileSet {
  Name = "PluginTest"
  Description = "Test the Plugin functionality with a Python Plugin."
  Include {
    Options {
      signature = MD5
    }
    Plugin = "@python_version@:module_path=@current_test_directory@/python-modules:module_name=bareos-fd-postgres:dbHost=@current_test_directory@/tmp:postgresDataDir=@current_test_directory@/database/data:walArchive=@current_test_directory@/database/wal_archive/"
  }
}
