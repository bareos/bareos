Client {
  Name = @basename@-fd
  Maximum Concurrent Jobs = 2000

  # remove comment from "Plugin Directory" to load plugins from specified directory.
  # if "Plugin Names" is defined, only the specified plugins will be loaded,
  # otherwise all filedaemon plugins (*-fd.so) from the "Plugin Directory".
  #
  # Plugin Directory = "@python_plugin_module_src_fd@"
  # Plugin Names = ""

  # if compatible is set to yes, we are compatible with bacula
  # if set to no, new bareos features are enabled which is the default
  # compatible = yes

  Working Directory =  "@wine_drive_prefix@@working_dir@"
  FD Port = @fd_port@
  MaximumBandwidthPerJob=10kb/s

}
