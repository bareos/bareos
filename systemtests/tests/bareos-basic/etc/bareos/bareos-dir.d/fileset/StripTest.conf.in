FileSet {
  Name = "StripTest"
  Description = "fileset just to backup some files for striptest"
  Include {
    Options {
      Signature = xxh128
      fstype = ext2
      fstype = ext3
      fstype = ext4
      fstype = overlay
      fstype = jfs
      fstype = ufs
      fstype = xfs
      fstype = zfs
      fstype = btrfs
      fstype = vfat
      StripPath = 1
    }
    File=<@tmpdir@/file-list
  }
}
