FileSet {
  Name = "IncusTestVM"
  Description = "Test the Incus Plugin with a virtual machine."
  Include {
    Options {
      Signature = XXH128
      Accurate = ipnugsamc
    }
   Plugin = "python"
        ":module_path=@python_plugin_module_src_test_dir@"
        ":module_name=bareos-fd-incus"
        ":instance=bareos-test-vm"
        ":compression=lzma"
        ":chunk_size=16MiB"
  }
}

