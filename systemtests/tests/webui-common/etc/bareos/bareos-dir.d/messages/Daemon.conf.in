Messages {
  Name = Daemon
  Description = "Message delivery for daemon messages (no job)."
  mail = @job_email@ = all, !skipped, !audit # (#02)
  console = all, !skipped, !saved, !audit
  append = "@wine_drive_prefix@@logdir@/bareos.log" = all, !skipped, !audit
  append = "@wine_drive_prefix@@logdir@/bareos-audit.log" = audit
}
