#
# Bareos User Agent (or Console) Configuration File
#

Director {
  Name = @basename@-dir
  DIRport = @dir_port@
  address = @hostname@
  Password = "@dir_password@"

  TLS Enable = yes
  TLS Require = yes
  TLS Verify Peer = no
  TLS Certificate = "@tls_con_certificate@"
  TLS Key = @tls_con_key@
  TLS CA Certificate File = "@tls_ca_certificate@"

}
