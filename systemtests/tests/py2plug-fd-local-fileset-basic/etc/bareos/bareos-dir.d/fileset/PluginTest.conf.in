FileSet {
  Name = "PluginTest"
  Description = "Test the Plugin functionality with a Python Plugin."

  Include {
    Options{
      acl support = yes
      xattr support = yes
    }
    Plugin = "@python_module_name@:module_path=@wine_drive_prefix@@python_plugin_module_src_test_dir@:module_name=bareos-fd-local-fileset-acl-xattr:filename=@wine_drive_prefix@@tmpdir@/file-list"
  }
}
