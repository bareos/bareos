JobDefs {
  Name = "DefaultJob"
  Type = Backup
  Level = Incremental
  Client = bareos-fd
  FileSet = "SelfTest-Pool"
  Storage = Normal
  Messages = Standard
  Priority = 10
  Write Bootstrap = "@working_dir@/%c.bsr"
  AllowDuplicateJobs = no
  CancelLowerLevelDuplicates = yes
  CancelQueuedDuplicates = yes
  CancelRunningDuplicates = no
}
