FileSet {
  Name = "PerconaXtraBackupTest"
  Description = "Test the Plugin functionality of the Percona Plugin."
  Include {
    Options {
      signature = MD5
    }
    Plugin = "@python_module_name@:module_path=@python_plugin_module_src_test_dir@:module_name=bareos-fd-percona-xtrabackup:dumpbinary=@XTRABACKUP@ --defaults-file=mysqldefaults:extradumpoptions=--user=root@extradumpoptions@:mysqlcmd=mysql --defaults-file=mysqldefaults --user=root --raw"
  }
}
