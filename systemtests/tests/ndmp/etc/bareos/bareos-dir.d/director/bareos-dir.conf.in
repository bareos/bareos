Director {                            # define myself
  Name = bareos-dir
  QueryFile = "@wine_drive_prefix@@scriptdir@/query.sql"
  Maximum Concurrent Jobs = 10
  Password = "@dir_password@"         # Console password
  Messages = Daemon
  Auditing = yes

  NDMP Log Level = 7
  NDMP Snooping = yes

  Backend Directory = @backenddir@
  Working Directory = "@wine_drive_prefix@@working_dir@"
  DirPort = @dir_port@
}
