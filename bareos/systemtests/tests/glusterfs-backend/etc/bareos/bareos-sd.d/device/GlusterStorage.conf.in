Device {
  Name = GlusterStorage
  Archive Device = "Gluster Device"
  Device Options = "uri=@glusterfs_uri@,loglevel=20,logfile=@logdir@/gluster.log"
  Device Type = gfapi
  Media Type = GlusterFile
  Label Media = yes
  Random Access = yes
  Automatic Mount = yes
  Removable Media = no
  Always Open = no
}
