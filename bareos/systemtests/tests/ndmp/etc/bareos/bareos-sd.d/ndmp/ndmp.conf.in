Ndmp {
  Name = bareos-dir-isilon                  # Can be any name but normally you should use the name of the Director here.
  Username = "@ndmp_data_agent_user@"       # Same username as you specified in the NDMPFile storage definition.
  Password = "@ndmp_data_agent_password@"   # Same password as you specified in the NDMPFile storage definition.
  AuthType = Clear                          # Clear == Clear Text, MD5 == Challenge protocol
}
