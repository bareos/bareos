@output nul
messages
@@out@ tmp/log1.out
run job=@JobName@
yes
wait
messages
@#
@# now do several restores to ensure we cleanup between jobs
@#
@output nul
restore where=@topdir@/tmp/bareos-restores select all storage=File done
yes
wait
restore where=@topdir@/tmp/bareos-restores select all storage=File done
yes
wait
@@out@ tmp/log2.out
restore where=@topdir@/tmp/bareos-restores select storage=File
unmark *
mark *
done
yes
wait
messages
@output
quit
