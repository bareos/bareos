@@out@ nul
messages
@@out@ tmp/log1.out
label storage=File1 volume=TestVolume001
label storage=File1 volume=TestVolume002
update Volume=TestVolume001 MaxVolBytes=3000000
run job=@JobName@ storage=File1 yes
wait
messages
@#
@# now do a restore
@#
@@out@ tmp/log2.out
restore bootstrap=@topdir@/working/restore.bsr where=@topdir@/tmp/bareos-restores select storage=File1
unmark *
cd @topdir@/build/src/cats
mark *
ls
done
yes
wait
messages
@@out@
quit
