	$(NO_ECHO)$(CXX) $(DEFS) $(DEBUG) -c $(WCFLAGS) $(CPPFLAGS) $(INCLUDES) $(DINCLUDE) $(OPENSSL_INCLUDES) $(GNUTLS_INCLUDES) $(CXXFLAGS) $<

